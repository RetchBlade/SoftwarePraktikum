<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Livable" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/own_very_dark_gray</item>
        <item name="colorPrimaryVariant">@color/own_very_dark_gray</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/own_grayish_yellow</item>
        <item name="colorSecondaryVariant">@color/own_navy_Blau</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:statusBarColor">@color/own_navy_Blau</item>
        <item name="buttonStyle">@style/CustomButtonStyle</item>
    </style>




    <style name="Theme.Livable.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Livable.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Livable.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
